return {
  'echasnovski/mini.icons',
  version = false, -- Use latest commit
  config = function()
    require('mini.icons').setup({
      style = 'glyph', -- or 'ascii' for fallback
      -- Custom overrides (optional)
      extension = {
        ['myext'] = { glyph = '󰻲', hl = 'MiniIconsRed' },
      },
      default = {
        file = { glyph = '󰈤' },
      },
      use_file_extension = function(ext)
        -- Example: ignore scm so it falls back to filetype match
        return ext ~= 'scm'
      end,
    })

    -- Optional: enable compatibility with plugins expecting nvim-web-devicons
    -- require('mini.icons').mock_nvim_web_devicons()

    -- Optional: tweak LSP kinds (e.g., in cmp)
    -- require('mini.icons').tweak_lsp_kind('prepend') -- or 'append'/'replace'
  end,
}
