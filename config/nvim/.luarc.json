{
  "runtime": {
    "version": "LuaJIT"
  },
  "diagnostics": {
    "globals": [
      "vim"
    ]
  },
  "workspace": {
    "library": [
      "${3rd}/luv/library",
      "${3rd}/busted/library",
      "/usr/share/nvim/runtime/lua",
      "/usr/share/nvim/runtime/lua/vim",
      "/your/path/to/nvim-config/lua"
    ],
    "checkThirdParty": false
  },
  "telemetry": {
    "enable": false
  }
}
